{
  "name": "mywebtest",
  "module": "src/server.tsx",
  "type": "module",
  "devDependencies": {
    "bun-types": "latest",
    "concurrently": "^8.2.2",
    "typed-html": "^3.0.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "bun run --watch src/server.tsx",
